{
  "name": "oea-token",
  "version": "1.0.0",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "start": "tsx ./src/index.ts",
    "dev": "nodemon -q",
    "lint": "eslint --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "cd .. && cd .. && husky install ./packages/oea-token/.husky"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx}": [
      "npm run lint",
      "npm run format"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@tatumio/tatum": "^4.2.40",
    "canvas": "^2.9.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "ethers": "^6.13.2",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "fs": "0.0.1-security",
    "http-status": "^1.7.4",
    "imagemin": "^8.0.1",
    "imagemin-pngquant": "^9.0.2",
    "qrcode": "^1.5.4",
    "tsx": "^4.19.2"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/http-status": "^0.2.30",
    "@types/imagemin": "^9.0.0",
    "@types/node": "^22.5.1",
    "@types/qrcode": "^1.5.5",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.9",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.2",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^5.5.4"
  }
}
